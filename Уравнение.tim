#Решение квадратного уравнения
использовать математика
а = число(ввод('Введи число а:'))
б = число(ввод('Введи число б:'))
с = число(ввод('Введи число с:'))

D = б**2 - 4*а*с

если D > 0:
    вывод("Два корня")
    вывод(f"{(математика.корень(D) + б)/(2**а)}")
    вывод(f"{(математика.корень(D) - б)/(2**а)}")
иначесли D равно 0:
    вывод('Один корень')
    вывод(f"{(матетматика.корень(D) + б)/(2**а)}")
иначе:
    вывод("Нет корней")